<div class="blog">
  <%= image_tag url_for(artwork_part.part_image) if artwork_part.part_image.attached? %>
</div>

<div class="blog">
    <%= artwork_part.text %>
</div>

<div class="blog control">
    <%= link_to 'Destroy', [artwork_part.artwork, artwork_part], 
        method: :delete, 
        data: { confirm: 'Are you sure?' } %>
</div>